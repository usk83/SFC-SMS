<p id="notice"><%= notice %></p>

<h1>Listing Allnights</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @allnights.each do |allnight| %>
      <tr>
        <td><%= allnight.date %></td>
        <td><%= link_to 'Show', allnight %></td>
        <td><%= link_to 'Edit', edit_allnight_path(allnight) %></td>
        <td><%= link_to 'Destroy', allnight, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Allnight', new_allnight_path %>
